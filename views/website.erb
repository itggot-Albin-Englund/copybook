<div class="notes"></div>
<div class="container">
    <div class="open" onclick="friendsOpen()">
        <a href="">Friends</a>
    </div>  
    <div class="friends">
        <a href="" onclick="friendsClose()">Close</a>
        <div class="list">
            <% a.each do |friend| %>
                <a href=""><%= friend[0] %></a>
            <% end %>
        </div>
    </div>
    <div class="add">
        <form action="/add" method="POST" autocomplete="off">
            <input type="text" name="name">
            <input type="submit" value="Add Friend">
        </form>
    </div>
    </div>
    <div class="chat">
        <div class="header">

        </div>
        <div class="window">

        </div>
        <div class="write">

        </div>
    </div>
</div>
<script type="text/javascript" src="./js/main.js"></script>